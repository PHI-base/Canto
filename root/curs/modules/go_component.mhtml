<%args>
$annotation_helper
</%args>

<div id="ontology-term-container">
<form action="" method="post">
  <div id="ontology-term-find">
  <input type="hidden" id="ontology-term-id"/>
  <span id="ontology-term-id-display"></span>
  <textarea id="ontology-term-entry" name="ontology-term-entry"
            type="textarea" value="" cols="30" rows="4"
            placeholder="start typing ..."></textarea>
  </div>
  <div id="ontology-term-children">(children)</div>
  <div id="ontology-term-definition">(definition)</div>
  <div class="submit">
    <input name="submit" type="submit" value="Submit" class="button" />
  </div>
</form>
</div>
